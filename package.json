{
  "name": "descobrindoclojure-vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "echo \"\\\"$(git describe --always)\\\"\" > src/assets/hash.json && vue-cli-service serve",
    "build": "echo \"\\\"$(git describe --always)\\\"\" > src/assets/hash.json && vue-cli-service build",
    "lint": "vue-cli-service lint",
